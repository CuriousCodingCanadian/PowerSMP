  ],
  "contact": {
    "repo": "https://github.com/CuriousCodingCanadian/PowerSMP"
  },
  "license": "GPL-3.0",
  "icon": "assets/powersmp/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "dev.ees4.powersmp.PowerSMP"
    ]
  },
  "mixins": [
    "powersmp.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
